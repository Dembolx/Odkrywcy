<Page x:Class="Odkrywcy_WorldMap.WorldMap"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Odkrywcy_WorldMap"
        mc:Ignorable="d"
        Title="Odkrywcy" Height="700" Width="1400">

    <Page.Resources>
        <!-- Złoty styl dla przycisków -->
        <Style TargetType="Button">
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Foreground" Value="Gold"/>
            <Setter Property="Background">
                <Setter.Value>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="#8B5A2B" Offset="0.0"/>
                        <GradientStop Color="#A67D3D" Offset="1.0"/>
                    </LinearGradientBrush>
                </Setter.Value>
            </Setter>
            <Setter Property="BorderBrush" Value="#D4AF37"/>
            <Setter Property="BorderThickness" Value="3"/>
            <Setter Property="Padding" Value="10"/>
            <Setter Property="Cursor" Value="Hand"/>

            <!-- Animacja blasku przy najechaniu -->
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Effect">
                        <Setter.Value>
                            <DropShadowEffect Color="Gold" BlurRadius="20"/>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </Style.Triggers>
        </Style>

        <Storyboard x:Key="FadeOutAnimation">
            <DoubleAnimation Storyboard.TargetProperty="Opacity"
                 From="1.0" To="0.0" Duration="0:0:0.5"
                 Completed="FadeOut_Completed"/>
        </Storyboard>

        <Storyboard x:Key="FadeInAnimation">
            <DoubleAnimation Storyboard.TargetProperty="Opacity"
                 From="0.0" To="1.0" Duration="0:0:0.5"/>
        </Storyboard>
    </Page.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <!-- Tytuł -->
            <Image x:Name="Tytul" Source="Images/odkrywcy.jpg" Grid.Row="0" Width="300" Margin="-50,0,50,0">
                <Image.RenderTransform>
                    <ScaleTransform ScaleX="1.5" ScaleY="1"/>
                </Image.RenderTransform>
            </Image>

            <!-- Przyciski -->
            <Button Content="Quiz Ogólny" Grid.Row="1" Click="Quiz_ogolny"/>
            <Button Content="Ranking" Grid.Row="2" Click="Ranking_Page"/>
            <Button Content="Sklep" Grid.Row="3" Click="Sklep_Page"/>
            <Button Content="Prezentacja" Grid.Row="4"/>
            <Button Content="Wyjdź" Grid.Row="5" Click="Wyjdz_Click"/>
        </Grid>

        <!-- Odtwarzacz wideo jako tło -->
        <MediaElement x:Name="BackgroundVideo" Source="Images/mapa_swiata.jpg"
          LoadedBehavior="Play" UnloadedBehavior="Manual"
          Stretch="Fill" MediaEnded="BackgroundVideo_MediaEnded" Grid.Column="1"/>


        <!-- Mapa Świata -->
        <Canvas x:Name="WorldMap_Canvas" Margin="0,0,147,0" Grid.Column="1">

